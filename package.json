    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^9.0.7",
    "jest": "^29.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
