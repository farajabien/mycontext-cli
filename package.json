{
  "name": "mycontext-cli",
  "version": "3.0.3",
  "description": "CLI tool for spec-driven development - Generate comprehensive context, visual screens, and scaffolding for AI-powered coding",
  "main": "dist/cli.js",
  "bin": {
    "mycontext-cli": "dist/cli.js",
    "mycontext": "dist/cli.js"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:compile && npm run build:alias && npm run build:copy",
    "build:clean": "rm -rf dist && ([ \"$npm_config_loglevel\" = \"silent\" ] || echo 'ðŸ§¹ Cleaned build directory')",
    "build:compile": "([ \"$npm_config_loglevel\" = \"silent\" ] || echo 'ðŸ”¨ Compiling TypeScript...') && tsc --noEmitOnError false",
    "build:alias": "([ \"$npm_config_loglevel\" = \"silent\" ] || echo 'ðŸ”— Resolving path aliases...') && tsc-alias",
    "build:copy": "([ \"$npm_config_loglevel\" = \"silent\" ] || echo 'ðŸ“‹ Copying config files...') && cp -r src/config dist/ && cp -r src/templates dist/ && cp package.json dist/ && ([ \"$npm_config_loglevel\" = \"silent\" ] || echo 'âœ… Build complete')",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "watch": "tsc --watch --pretty",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:integration": "jest --testPathPattern=integration --passWithNoTests",
    "test:unit": "jest --testPathPattern=unit --passWithNoTests",
    "lint": "eslint src/**/*.ts --quiet",
    "lint:fix": "eslint src/**/*.ts --fix --quiet",
    "prepublishOnly": "echo 'Skipping tests and lint for now' && npm run build"
  },
  "keywords": [
    "mycontext",
    "mycontext-ai",
    "mycontext-cli",
    "mycontext-components",
    "ai",
    "components",
    "shadcn",
    "nextjs",
    "typescript",
    "react"
  ],
  "author": "MyContext",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farajabien/mycontext-cli.git"
  },
  "homepage": "https://github.com/farajabien/mycontext-cli#readme",
  "bugs": {
    "url": "https://github.com/farajabien/mycontext-cli/issues"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.1",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@huggingface/inference": "^4.11.1",
    "@types/figlet": "^1.7.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "diff": "^8.0.2",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "figlet": "^1.9.3",
    "fs-extra": "^11.1.1",
    "fuse.js": "^7.1.0",
    "glob": "^10.3.10",
    "gradient-string": "^3.0.0",
    "inquirer": "^9.2.12",
    "mdast-util-to-markdown": "^2.1.2",
    "node-fetch": "^2.7.0",
    "openai": "^6.2.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "remark-parse": "^11.0.0",
    "sonner": "^2.0.7",
    "unified": "^11.0.5",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
