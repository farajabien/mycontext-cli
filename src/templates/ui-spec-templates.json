        "prominence": "high"
      },
      {
        "type": "navItem",
        "label": "Home",
        "active": true,
        "prominence": "medium"
      },
      {
        "type": "navItem",
        "label": "About",
        "prominence": "medium"
      },
      {
        "type": "navItem",
        "label": "Services",
        "prominence": "medium"
      },
      {
        "type": "navItem",
        "label": "Contact",
        "prominence": "medium"
      }
    ],
    "layout": "horizontal",
    "styling": {
      "theme": "modern",
      "colors": ["primary", "secondary"],
      "spacing": "medium",
      "borderRadius": "none",
      "shadow": "sm",
      "alignment": "space-between"
    }
